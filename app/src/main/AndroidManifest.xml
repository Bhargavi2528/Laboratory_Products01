<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cssolution.labproduct">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/cscslogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cscslogo"
        android:supportsRtl="true"
        android:largeHeap="true"
        android:hardwareAccelerated="false"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.LaboratoryProduct">
        <activity android:name=".DashboardModule.activity.TestCommonActivity"></activity>
        <activity android:name=".DashboardModule.activity.CommonTabActivity" />
        <activity android:name=".DashboardModule.activity.AddAddressActivity" />
        <activity android:name=".DashboardModule.activity.AddMemberActivity" />
        <activity android:name=".DashboardModule.activity.PackageDetailsActivity" />
        <activity android:name=".DashboardModule.activity.PackageActivity" />
        <activity android:name=".DashboardModule.activity.UploadPrescriptionActivity" />
        <activity android:name=".DashboardModule.activity.BookTestActivity" />
        <activity android:name=".DashboardModule.activity.RefundPolicyActivity" />
        <activity android:name=".DashboardModule.activity.EditProfileActivity" />
        <activity android:name=".DashboardModule.activity.NotificationActivity" />
        <activity android:name=".DashboardModule.activity.MyAddressActivity" />
        <activity android:name=".DashboardModule.activity.MyFamilyActivity" />
        <activity android:name=".DashboardModule.activity.MyOrderActivity" />
        <activity android:name=".DashboardModule.activity.ContactusActivity" />
        <activity android:name=".DashboardModule.activity.MainDashboardActivity" />
        <activity android:name=".LoginModule.RegisterActivity" />
        <activity android:name=".LoginModule.LoginActivity" />
        <activity android:name=".LoginModule.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>